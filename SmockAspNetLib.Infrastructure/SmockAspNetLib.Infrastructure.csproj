<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <Company>Smock</Company>
    <Product>SmockAspNetLib</Product>
    <Authors>Smock</Authors>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Copyright />
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageId>SmockAspNetLib</PackageId>
    <Description>smock的一些常用基础库</Description>
    <PackageReleaseNotes>支持3.0</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/smockgithub/SmockAspNetLib</RepositoryUrl>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <FileVersion>1.0.8.0</FileVersion>
    <AssemblyVersion>1.0.8.0</AssemblyVersion>
    <Version>1.0.8</Version>
    <RunPostBuildEvent>OnBuildSuccess</RunPostBuildEvent>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net461|AnyCPU'">
    <DocumentationFile />
  </PropertyGroup>



  <ItemGroup>
    <Folder Include="Attributes\" />
  </ItemGroup>

</Project>
